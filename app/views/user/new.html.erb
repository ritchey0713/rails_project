<h1> Thanks for signing up with us! </h1>

<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :console %>
  <%= f.text_field :console %>

  <%= f.label :goal %>
  <%= f.number_field :goal %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Create Account!" %>
